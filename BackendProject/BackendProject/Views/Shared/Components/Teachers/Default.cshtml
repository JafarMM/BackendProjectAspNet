@model List<Teacher>

@foreach (Teacher item in Model)
{

        <div class="col-md-3 col-sm-4 col-xs-12">
            <div class="single-teacher mb-45">
                <div class="single-teacher-img">
                    <a href="teacher-details.html"><img src="img/teacher/@item.Image" alt="teacher"></a>
                </div>
                <div class="single-teacher-content text-center">
                    <h2><a href="teacher-details.html">@item.Name</a></h2>
                    <h4>Associate Professor</h4>
                    <ul>
                        <li><a href="@item.FacebookProfile"><i class="zmdi zmdi-facebook"></i></a></li>
                        <li><a href="@item.PinterestProfile"><i class="zmdi zmdi-pinterest"></i></a></li>
                        <li><a href="@item.VimeProfile"><i class="zmdi zmdi-vimeo"></i></a></li>
                        <li><a href="@item.TwitterProfile"><i class="zmdi zmdi-twitter"></i></a></li>
                    </ul>
                </div>
            </div>
        </div>
  
}

