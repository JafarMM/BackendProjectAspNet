@model Bio 

    <header class="top">
        <div class="header-top">
            <div class="container">
                <div class="row">
                    <div class="col-md-8 col-sm-8 col-xs-12">
                        <div class="header-top-left">
                            <p>HAVE ANY QUESTION ?  +@Model.Number</p>
                        </div>
                    </div>
                    <div class="col-md-4 col-sm-4 col-xs-12">
                        <div class="header-top-right text-right">
                            <ul>
                                <li><a asp-controller="Account" asp-action="Login">login</a></li>
                                <li><a asp-controller="Account" asp-action="Register">signup</a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="header-area two header-sticky">
            <div class="container">
                <div class="row">
                    <div class="col-md-3 col-sm-3 col-xs-6">
                        <div class="logo">
                            <a asp-controller="Home" asp-action="Index"><img src="img/logo/@Model.Logo" alt="eduhome" /></a>
                        </div>
                    </div>
                    <div class="col-md-9 col-sm-9 col-xs-6">
                        <div class="content-wrapper text-right">
                            <!-- Main Menu Start -->
                            <div class="main-menu">
                                <nav>
                                    <ul>
                                        <li>
                                            <a asp-controller="Home" asp-action="Index">Home</a>
                                        </li>
                                        <li><a asp-controller="About" asp-action="Index">About</a></li>
                                        <li>
                                            <a asp-controller="Courses" asp-action="Index">courses</a>
                                        </li>
                                        <li>
                                            <a asp-controller="Event" asp-action="Index">events</a>
                                        </li>
                                        <li class="hidden-sm">
                                            <a asp-controller="Teacher" asp-action="Index">teachers</a>
                                        </li>
                                        <li>
                                            <a href="blog.html">blogs</a>
                                        </li>
                                        <li><a href="contact.html">Contact</a></li>
                                        <li><a href="#">Buy Now</a>
                                    </ul>
                                </nav>
                            </div>
                            <!--Search Form Start-->
                            <div class="search-btn">
                                <ul data-toggle="dropdown" class="header-search search-toggle">
                                    <li class="search-menu">
                                        <i class="fa fa-search"></i>
                                    </li>
                                </ul>
                                <div class="search">
                                    <div class="search-form">
                                            <i class="fas fa-search" id="search"></i>
                                            <ul class="list-unstyled py-3" id="searchResult">
                                                <li class="d-flex px-3">
                                                    <input id="input-search" type="text" placeholder="Search">
                                                    <i class="fas fa-search my-auto" id="search"></i>
                                                </li>
                                            </ul>    
                                    </div>
                                </div>
                            </div>
                            <!--End of Search Form-->
                            <!-- Main Menu End -->
                        </div>
                    </div>
                    <div class="col-xs-12">
                        <div class="mobile-menu hidden-lg hidden-md hidden-sm"></div>
                    </div>
                </div>
            </div>
        </div>
    </header>